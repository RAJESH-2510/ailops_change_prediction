{
  "dashboard": {
    "title": "AI Ops Change Failure Prediction",
    "panels": [
      {
        "id": 1,
        "title": "Real-time Risk Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "risk_score",
            "format": "time_series"
          }
        ],
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "value": 0, "color": "green" },
            { "value": 0.4, "color": "yellow" },
            { "value": 0.7, "color": "red" }
          ]
        }
      },
      {
        "id": 2,
        "title": "Deployment Success Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(successful_deployments[5m])",
            "legendFormat": "Success Rate"
          }
        ]
      }
    ]
  }
}
